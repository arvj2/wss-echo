<definitions 
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    xmlns:wsam="http://www.w3.org/2005/07/addressing/metadata"
    targetNamespace="http://com.security/port"
    xmlns:tn="http://com.security/port"
    xmlns:types="http://com.security/types"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"           
>
    
    <types>
        <xs:schema>
            <xs:import namespace="http://com.security/types" schemaLocation="types.xsd"/>
        </xs:schema> 
    </types>
    
    <message name="Echo">
       <part name="echoed" element="types:Echo"/>
    </message>
    
    
    <message name="EchoResponse">
       <part name="value" element="types:EchoResponse"/>
    </message>    
    
    <portType name="EchoPort">
        <operation name="Echo">
            <input message="tn:Echo" wsam:Action="http://com.security/port/EchoRequest"/>              
            <output message="tn:EchoResponse" wsam:Action="http://com.security/port/EchoResponse"/>
        </operation>
    </portType>
    
</definitions>