<?xml version="1.0"?>
<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"           
           elementFormDefault="qualified"
           targetNamespace="http://com.security/types"
           xmlns:tn="http://com.security/types">

    
    <xs:element name="Echo" type="tn:EchoType"/>
    <xs:element name="EchoResponse" type="tn:EchoResponseType"/>
    
    
    <xs:complexType name="EchoType">
        <xs:sequence>
            <xs:element name="echoed" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    
        <xs:complexType name="EchoResponseType">
        <xs:sequence>
            <xs:element name="value" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
        
</xs:schema>
